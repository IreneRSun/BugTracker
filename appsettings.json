{
  "ConnectionStrings": {
    "DefaultConnection": "server={YOUR_MYSQL_SERVER};port={yOUR_PORT_NUMBER};database=bug_tracker;user={yOUR_MYSQL_USER};password={yOUR_MYSQL_PASSWORD}"
  },
  "Auth0": {
